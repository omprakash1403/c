#include<stdio.h>
struct Palindrome
{
    int letter,rev;
    int n;
    char a[100];
};
struct Palindrome input_data()
{
    struct  Palindrome p;
    printf("enter the no of time to be done");
    scanf("%d",&p.n);
    for(int i=0;i<p.n;i++)
        {
            printf("enter the words to be printed");
            scanf("%s",&p.a[100]);
            
        while(a=!0)
        {
        letter=a%10;
        rev=rev*10+letter;
        a=a/10;
        }
        }
    return(p);
}
    
void output_data(struct Palindrome p)
{
    printf("%d",p.a[100]);
}
int main()
{
    
    
    struct Palindrome p;
    p=input_data();
    output_data(p);
    
}
